{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "LambdaBucketLevel",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::id-conta-linked:role/service-role/agent-finops-ia-role-3z2rmqfr"
            },
            "Action": [
                "s3:GetBucketLocation",
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::ia-tmp-athena-staging"
        },
        {
            "Sid": "LambdaObjectLevel",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::id-conta-linked:role/service-role/agent-finops-ia-role-3z2rmqfr"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": "arn:aws:s3:::ia-tmp-athena-staging/*"
        },
        {
            "Sid": "AthenaBucketLevel",
            "Effect": "Allow",
            "Principal": {
                "Service": "athena.amazonaws.com"
            },
            "Action": [
                "s3:GetBucketLocation",
                "s3:ListBucket",
                "s3:ListBucketMultipartUploads"
            ],
            "Resource": "arn:aws:s3:::ia-tmp-athena-staging"
        },
        {
            "Sid": "AthenaObjectLevel",
            "Effect": "Allow",
            "Principal": {
                "Service": "athena.amazonaws.com"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:AbortMultipartUpload",
                "s3:ListMultipartUploadParts"
            ],
            "Resource": "arn:aws:s3:::ia-tmp-athena-staging/*"
        }
    ]
}